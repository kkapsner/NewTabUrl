{
  "name": "newtaburl",
  "version": "1.0.0",
  "description": "Web-Extension to control the new tab page",
  "main": "newTab.js",
  "scripts": {
    "run current": "web-ext run",
    "run beta": "web-ext run -f firefox-beta",
    "run": "web-ext run -f firefox-nightly",
    "run esr": "web-ext run -f firefox-esr",
    "build": "web-ext build --overwrite-dest --ignore-files versions --ignore-files package*",
    "eslint": "eslint ./ --ext .js,.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkapsner/NewTabUrl.git"
  },
  "keywords": [
    "web-extension",
    "web-ext",
    "browser",
    "new",
    "tab",
    "customization"
  ],
  "author": "Korbinian Kapsner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kkapsner/NewTabUrl/issues"
  },
  "homepage": "https://github.com/kkapsner/NewTabUrl#readme",
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "web-ext": "^7.6.2"
  }
}
